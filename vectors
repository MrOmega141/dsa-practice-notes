#include <iostream>
#include<vector>
using namespace std;

int main() {
vector<int> v;
v.push_back(1);
cout<<"the size is ==="<<v.capacity()<<endl;//shows the capacity
v.push_back(2);
cout<<"the size is ==="<<v.capacity()<<endl;
v.push_back(3);
cout<<"the size is ==="<<v.capacity()<<endl;
v.push_back(257);
cout<<"the size is ==="<<v.capacity()<<endl;
cout<<"the first element is -->"<<v.front()<<endl;//first element
cout<<"the last element is -->"<<v.back()<<endl;//last element



	return 0;
}
